<table>
    <tr>
        <td>ID</td>
        <td>Nombre</td>
        <td>Email</td>
        <td></td>
        <td></td>
        <td></td>
    </tr>


    <% _.each(users, function(user){ %>

    <tr>
        <td><%= user.id %></td>
        <td><%= user.nombre %></td>
        <td><%= user.email %></td>
        <td><a href="/foo/show/<%= user.id %>">Ver</a> </td>
        <td><a href="/foo/edit/<%= user.id %>">Editar</a></td>
        <td><a href="/foo/destroy/<%= user.id %>">Eliminar</a></td>
    </tr>

    <% }); %>

</table>